<Page x:Class="RQuote.TestPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RQuote"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="TestPage">

    <Grid Background="Wheat">
        <!--<local:SideMenuControl x:Name="SideMenu" >
            
        </local:SideMenuControl>-->
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="60"></ColumnDefinition>
            <ColumnDefinition ></ColumnDefinition>
            <ColumnDefinition Width="60"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Grid Margin="0 0 0 0" Grid.Row="0" Grid.RowSpan="2">
            <local:SpacedLabel Padding="0 12 12 0" FontFamily="/RQuote;component/Fonts/#Lato" FontSize="14" Foreground="#868686" Text="Rousing, Bangalore-37"></local:SpacedLabel>
        </Grid>
    </Grid>
</Page>
